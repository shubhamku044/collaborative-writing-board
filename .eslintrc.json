{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-empty-function": "off",
    "semi": ["error", "always"],
    "jsx-quotes": ["error", "prefer-double"],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxBOF": 1
      }
    ]
  }
}
